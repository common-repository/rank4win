<div id="r4w_st_page_export_import" class="page_tab"> 
	<div id="r4w_box_subtabs" class="box_subtab">
		<ul>
			<li id="subpage_export" class="btn_subtab">{_('Export')}</li>
			<li id="subpage_import" class="btn_subtab">{_('Import')}</li>
		</ul>
	</div>
	<div id="r4w_box_subpages">

		<div id="r4w_sst_subpage_export" class="page_subtab">
			<label class="css-gfejhyi850 title_gwp">
				{_('Export parameter configuration')}
			</label>
			<div class="r4w_bullet_info">
				<div class="css-sd5r0fze5">[@svg_bullet_info]</div>
				<div class="css-df5r0grg">
					{_('This option allows you to export the ran4win plugin settings, for a backup or to be imported on another wordpress')}.
				</div>
			</div>
			[@rs_importexport_txt]
			[@tools_export_btn]
		</div>
		<div id="r4w_sst_subpage_import" class="page_subtab">
			<label class="css-gfejhyi850 title_gwp">
				{_('Import parameter configuration')}
			</label>
			<div class="r4w_bullet_info">
				<div class="css-sd5r0fze5">[@svg_bullet_info]</div>
				<div class="css-df5r0grg">
					{_('This option allows you to import your settings that were exported by Rank4Win. Always try to have the same version between the export and import of the setting file')}.
				</div>
			</div>
			[@rs_importexport_txt]
			[@tools_import_input]
			[@tools_import_btn]
		</div>
	</div>	
</div>